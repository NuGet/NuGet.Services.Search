<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="NuGet.Services.SecureSearch.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-06.2.4">
  <WebRole name="NuGet.Services.SecureSearch" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <ConfigurationSettings>
      <Setting name="Search.RegistrationBaseAddress" />
      <Setting name="Local.Lucene.Directory" />
      <Setting name="Search.IndexRefresh" />
      <Setting name="Storage.Primary" />
      <Setting name="Search.IndexContainer" />
      <Setting name="ida.Tenant" />
      <Setting name="ida.Audience" />
      <Setting name="ida.ClientId" />
      <Setting name="ida.AADInstance" />
    </ConfigurationSettings>
  </WebRole>
</ServiceDefinition>